{
  "name": "sirene-invader",
  "version": "1.0.0",
  "description": "Efficient MongoDB file indexing",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.json",
    "dev": "NODE_ENV=development tsnd --inspect --respawn src/main.ts --watch src/**",
    "start": "yarn build && node ./dist/main.js",
    "start:debug": "yarn build && node --inspect --trace-warnings ./dist/main.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lvndry/sirene-invader.git"
  },
  "keywords": [],
  "author": "lvndry",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lvndry/sirene-invader/issues"
  },
  "homepage": "https://github.com/lvndry/sirene-invader#readme",
  "dependencies": {
    "mongoose": "^6.0.11",
    "ts-node": "^10.3.0"
  },
  "devDependencies": {
    "@types/node": "^16.11.1",
    "rimraf": "^3.0.2",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.4"
  }
}
